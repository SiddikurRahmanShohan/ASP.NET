@model List<BusinessLayer.Entities.StudentEM>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div>
    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Department Id</th>

        </tr>
        @foreach (var s in Model)
        {
            <tr>
                <td>@s.Id</td>
                <td>@s.Name</td>
                <td>@s.DeptId</td>
            </tr>
        }

    </table>
</div>